{
  "name": "next-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watchAll",
    "test:coverage": "jest",
    "lint:eslint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx}\" --max-warnings=-1",
    "lint:tsc": "tsc --project ./tsconfig.lint.json --noEmit",
    "lint": "npm run lint:eslint && npm run lint:tsc",
    "lint:staged": "lint-staged -c .lintstagedrc.cjs",
    "prettier:check": "prettier -c \"./src/**/*.{js,ts,tsx}\"",
    "prettier:fix": "prettier --write \"./src/**/*.{js,ts,tsx}\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "classnames": "^2.3.1",
    "antd": "^4.23.2",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "next": "12.3.1",
    "next-with-less": "^2.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.4",
    "sass": "^1.55.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "18.7.23",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "typescript": "4.8.3",
    "eslint-config-next": "12.3.1",
    "eslint": "8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
